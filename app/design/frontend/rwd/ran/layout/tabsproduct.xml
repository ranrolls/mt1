			<action method="addCss" ifconfig="tabsproduct/tabsproduct_config/enabled">
				<stylesheet>magentothem/css/ma.tabsproduct.css</stylesheet>
			</action>
			<action method="addJs" ifconfig="tabsproduct/tabsproduct_config/enabled">
